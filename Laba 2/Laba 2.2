#include <stdio.h>

int main()
{
	int a;
	int i = 0;
	int sum = 0;
	int sum1 = 1;
	printf("Input number a = ");
	scanf_s("%d",&a);
	for (i = 1; i <= a; i++)
	{
		if (i % 3 == 0)
		{
			sum1 *= i;
		}
	}
	printf("Multiplication = %d\n", sum1);
	
	for (i = 1; i <= a; i++)
	{
		if (i % 3 == 0)
		{
			sum += i;
		}
	}
	printf("Sum = %d\n", sum);
	return 0;
}	
